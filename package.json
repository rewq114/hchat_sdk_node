{
  "name": "@rewq114/h-chat-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Hyundai Motor Group H-Chat API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rewq114/hchat_sdk_node.git"
  },
  "keywords": [
    "hchat",
    "hyundai",
    "ai",
    "chatbot",
    "sdk",
    "typescript"
  ],
  "author": "rewq114",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:quick": "ts-node test.ts",
    "example:basic": "ts-node examples/basic.ts",
    "example:tool": "ts-node examples/tool-use.ts",
    "example:stream-tool": "ts-node examples/stream-tool-use.ts",
    "example:manual": "ts-node examples/manual-mode.ts",
    "example:agent": "ts-node examples/agent-mode.ts",
    "example:chat": "ts-node examples/update-chat.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.59.0",
    "openai": "^5.12.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "dotenv": "^17.2.1",
    "jest": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "*"
  }
}
